---
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml" />
    {% for spec in site.intent_specs %}
      <item>
        <title>{{ spec.title | xml_escape }}</title>
        <description>{{ spec.content | xml_escape }}</description>
        <pubDate>{{ spec.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <link>{{ spec.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ spec.url | prepend: site.baseurl | prepend: site.url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
